project boost/cgi/example/cgi/hello_world ;

exe cgi_hello_world
  :
     main.cpp /boost/regex/ /boost/cgi/
  :
  ;


# Our install rule (builds binaries and copies them to <location>)
install install
 : cgi_hello_world
 : <location>$(cgi-bin)
 ;

# Only install example if you use `bjam install' or equivalent
explicit install ;
